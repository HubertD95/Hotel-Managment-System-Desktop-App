<Window x:Class="inz_vol._2.LogiWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:inz_vol._2"
        mc:Ignorable="d"
        Title="Logi" Height="300" Width="705.544"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <ListView x:Name="ListViewRezerwacje"  HorizontalAlignment="Left" Height="226" Margin="10,33,0,0" VerticalAlignment="Top" Width="678">
            <ListView.View>
                <GridView>
                    <GridViewColumn DisplayMemberBinding="{Binding Log}">
                        
                    </GridViewColumn>
                </GridView>
            </ListView.View>
        </ListView>
        <Label Content="Wyszukaj po frazie:" HorizontalAlignment="Left" Margin="135,7,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.414,-0.055"/>
        <TextBox x:Name="TB_wyszukaj" HorizontalAlignment="Left" Height="23" Margin="251,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="308" TextChanged="TB_wyszukajTextChange"/>

    </Grid>
</Window>
